msgid ""
"SSH server is enabled by default.  Login with username: <b>root</b> and "
"password: <b>openmediavault</b>"
msgstr "SSH服务默认开启，登录用户名： <b>root</b>，密码：<b>openmediavault</b>"

msgid ""
"SSH server is enabled by default.  Login with username: <b>root</b> and "
"password: <b>partedmagic</b>"
msgstr "SSH服务默认开启，登录用户名： <b>root</b>，密码：<b>partedmagic</b>"

msgid ""
"SSH server is enabled by default.  Login with username: <b>user</b> and "
"password: <b>live</b>"
msgstr "SSH服务默认开启，登录用户名： <b>user</b>，密码：<b>live</b>"

